<Window x:Class="JSON_Game_of_Thrones_Quote.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JSON_Game_of_Thrones_Quote"
        mc:Ignorable="d"
        Title="Game of Thrones Quotes" Height="450" Width="800">
    <Window.Background>
        <ImageBrush ImageSource="https://th.bing.com/th/id/OIP.vZXMLWcZWoKLwSCzd9fDugHaEK?w=311&amp;h=180&amp;c=7&amp;r=0&amp;o=5&amp;dpr=1.5&amp;pid=1.7"/>
    </Window.Background>
    <Grid>
        <TextBox x:Name="txtQuote" TextWrapping="Wrap" Margin="142,0,138,286" Background="#7FFFFFFF" FontSize="18" IsEnabled="False"/>
        <Button x:Name="btnGet" Content="Get Quote" HorizontalAlignment="Left" Margin="24,39,0,0" VerticalAlignment="Top" Height="60" Width="91" Background="#CCDDDDDD" Click="btnGet_Click"/>
        <Button x:Name="btnSave" Content="Save Quotes" HorizontalAlignment="Left" Margin="674,34,0,0" VerticalAlignment="Top" Height="60" Width="91" Background="#CCDDDDDD" Click="btnSave_Click"/>

    </Grid>
</Window>
